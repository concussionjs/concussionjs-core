<script>
	console.log('logging out');
	var URLPrefix ="api.concussionjs.com";
	var token = localStorage.getItem("token");
	
	document.location = "http://m.facebook.com/logout.php?confirm=1&next=http://<%=CJS_WEB_URL%>/logout&access_token=" + token;

